.previous_next {
  width: 100%;
  height: 110rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-shadow: 0 -7rpx 20rpx #dbdfe4;
}
.previous_next .btn {
  width: 374.5rpx;
  color: #333333;
  font-size: 30rpx;
  line-height: 110rpx;
  text-align: center;
  letter-spacing: 3rpx;
}
.previous_next .blank {
  width: 1rpx;
  height: 42rpx;
  background-color: #9C9C9C;
  margin: 34rpx 0;
}

