.swiper_container {
  position: relative;
}
.swiper_container .swiper {
  width: 100%;
  height: 294rpx;
  margin: 20rpx 0rpx;
  background-color: #FFFFFF;
}
.swiper_container .swiper swiper-item .swiper-item {
  height: 294rpx;
  margin: 0 10rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-radius: 10rpx;
}
.swiper_container .swiper swiper-item .swiper-item image {
  width: 690rpx;
  height: 294rpx;
  background-color: #ffffff;
  border-radius: 10rpx;
}

